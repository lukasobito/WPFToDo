<Window x:Class="WPFToDo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFToDo"
        xmlns:vm="clr-namespace:WPFToDo.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" DataContext="{StaticResource Main}">
    <Grid>
        <DataGrid ItemsSource="{Binding Items}" AutoGenerateColumns="true">
        </DataGrid>
    </Grid>
</Window>
